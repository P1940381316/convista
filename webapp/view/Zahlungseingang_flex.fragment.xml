						<ui.com:dimensionSelectors>
							<Select id="idoSelectEing" change="handleSelectionChangeLastschrift">
							    <core:Item key="0" text="Alle"/>
								<core:Item key="1" text="Lastschrift"/>
								<core:Item key="2" text="Kreditkarte"/>
								<core:Item key="3" text="Verrechnungscheck"/>
								<core:Item key="4" text="PayPal"/>
							</Select>
						</ui.com:dimensionSelectors>
